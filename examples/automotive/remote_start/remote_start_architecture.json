<?xml version="1.0" encoding="UTF-8"?>
<capella:Project xmlns:capella="http://www.polarsys.org/capella/core/1.4.0">
  <ownedRequirements>
    <requirement id="STK-RS-001" name="STK-RS-001" description="User must be able to remotely start the vehicle from their smartphone within 100m range" priority="Critical" />
    <requirement id="STK-RS-002" name="STK-RS-002" description="System must prevent remote start if vehicle safety conditions are not met" priority="Critical" />
    <requirement id="STK-RS-003" name="STK-RS-003" description="Remote start must work for ICE, Hybrid, and Electric vehicles with appropriate powertrain control" priority="Critical" />
    <requirement id="STK-RS-004" name="STK-RS-004" description="System must authenticate user and secure all communications to prevent unauthorized access" priority="Critical" />
    <requirement id="STK-RS-005" name="STK-RS-005" description="Vehicle must comply with regional emission and safety regulations during remote start" priority="High" />
    <requirement id="STK-RS-006" name="STK-RS-006" description="User shall receive confirmation of remote start status within 3 seconds" priority="High" />
    <requirement id="STK-RS-007" name="STK-RS-007" description="System must automatically shut down after 10 minutes if not manually overridden" priority="Medium" />
    <requirement id="STK-RS-008" name="STK-RS-008" description="Remote start must maintain cabin comfort through climate control pre-conditioning" priority="Medium" />
    <requirement id="SYS-RS-001" name="SYS-RS-001" description="System shall authenticate user identity using cryptographic tokens before allowing remote start" priority="Critical" />
    <requirement id="SYS-RS-002" name="SYS-RS-002" description="System shall verify all safety interlocks (parking brake, neutral, doors closed) before starting" priority="Critical" />
    <requirement id="SYS-RS-003" name="SYS-RS-003" description="System shall establish secure encrypted communication channel between smartphone and vehicle" priority="Critical" />
    <requirement id="SYS-RS-004" name="SYS-RS-004" description="System shall monitor battery state of charge and prevent start if below 20 percent for EV or voltage below 11V for ICE" priority="High" />
    <requirement id="SYS-RS-005" name="SYS-RS-005" description="System shall activate appropriate powertrain control strategy based on vehicle type ICE Hybrid or EV" priority="Critical" />
    <requirement id="SYS-RS-006" name="SYS-RS-006" description="System shall send status notifications success failure warnings to user smartphone" priority="High" />
    <requirement id="SYS-RS-007" name="SYS-RS-007" description="System shall implement automatic shutdown timer with maximum 10 minute duration" priority="High" />
    <requirement id="SYS-RS-008" name="SYS-RS-008" description="System shall activate climate control to target temperature based on user preferences" priority="Medium" />
    <requirement id="SYS-RS-009" name="SYS-RS-009" description="System shall log all remote start attempts with timestamp and result for audit trail" priority="Medium" />
    <requirement id="SYS-RS-010" name="SYS-RS-010" description="System shall detect tampering or replay attacks and raise security alerts" priority="Critical" />
    <requirement id="FUNC-RS-001" name="FUNC-RS-001" description="Remote start request processing function shall validate user credentials within 500ms" priority="Critical" />
    <requirement id="FUNC-RS-002" name="FUNC-RS-002" description="Safety interlock validation function shall check all preconditions within 200ms" priority="Critical" />
    <requirement id="FUNC-RS-003" name="FUNC-RS-003" description="Powertrain control function shall initiate start sequence based on vehicle type" priority="Critical" />
    <requirement id="FUNC-RS-004" name="FUNC-RS-004" description="Notification service shall deliver status updates with 99.9 percent reliability" priority="High" />
    <requirement id="SAFE-RS-001" name="SAFE-RS-001" description="Remote start controller shall implement watchdog monitoring with 100ms timeout" priority="Critical" />
    <requirement id="SAFE-RS-002" name="SAFE-RS-002" description="Safety interlock failure shall inhibit remote start and log fault code" priority="Critical" />
    <requirement id="SAFE-RS-003" name="SAFE-RS-003" description="Remote start shall be inhibited if any ASIL B or higher fault is active" priority="Critical" />
    <requirement id="SEC-RS-001" name="SEC-RS-001" description="All remote commands shall use AES 256 encryption with perfect forward secrecy" priority="Critical" />
    <requirement id="SEC-RS-002" name="SEC-RS-002" description="System shall implement certificate based authentication with hardware backed key storage" priority="Critical" />
    <requirement id="SEC-RS-003" name="SEC-RS-003" description="Replay attack detection shall use cryptographic nonce with maximum 5 second window" priority="Critical" />
    <requirement id="SEC-RS-004" name="SEC-RS-004" description="Failed authentication attempts shall trigger exponential backoff after 3 failures" priority="High" />
    <requirement id="REG-RS-001" name="REG-RS-001" description="System shall comply with FCC Part 15 for radiated emissions" priority="Critical" />
    <requirement id="REG-RS-002" name="REG-RS-002" description="Remote start duration shall comply with local idle time regulations maximum 10 minutes" priority="High" />
    <requirement id="REG-RS-003" name="REG-RS-003" description="System shall comply with GDPR for user data privacy and consent" priority="High" />
    <requirement id="REG-RS-004" name="REG-RS-004" description="Remote start shall comply with UNECE R100 for electric vehicle safety" priority="High" />
  </ownedRequirements>
