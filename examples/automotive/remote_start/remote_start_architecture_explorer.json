          "category": null,
          "safety_level": null,
          "traces_to": []
        },
        {
          "id": "FUNC-RS-002",
          "description": "Safety interlock validation function shall check all preconditions within 200ms",
          "priority": "Critical",
          "category": null,
          "safety_level": "ASIL_B",
          "traces_to": []
        },
        {
          "id": "FUNC-RS-003",
